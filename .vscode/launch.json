  "version": "0.2.0",
  "configurations": [
    {
      "name": "api-Launch",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/apiMemo-MongoDB",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "api-compile",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/apiMemo-MongoDB",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["compile"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
